CREATE TABLE `estate`.`assets_report` (
  `id` int(5) NOT NULL,
  `projectName` varchar(60) NOT NULL,
  `assessReportNum` varchar(40) DEFAULT NULL,
  `assessStartTime` date DEFAULT NULL,
  `assessEndTime` date DEFAULT NULL,
  `assessObject` varchar(60) DEFAULT NULL,
  `valueTime` date DEFAULT NULL,
  `assessAim` varchar(10) DEFAULT NULL,
  `assessMethod` varchar(10) DEFAULT NULL,
  `reportType` varchar(10) DEFAULT NULL,
  `branchOffice` varchar(10) DEFAULT NULL,
   `upFileURI` varchar(80) DEFAULT NULL COMMENT '上传文件的URI',
   
  `assessedUnitName` varchar(20) DEFAULT null COMMENT '被评估单位名称',
    `isStateAssets` boolean DEFAULT false COMMENT '是否国有资产',
  `isPrivateAsset` boolean DEFAULT false COMMENT '是否私有资产',
  `valueType` VARCHAR(10) DEFAULT null COMMENT '价值类型',
  `assessResult` float(10,3) DEFAULT null COMMENT '评估结论',
  `assessDate` date DEFAULT null COMMENT '评估基准日',
   `assetsReportDate` date DEFAULT null COMMENT '资产评估报告日',
    `actualFee` float(10,3) DEFAULT null COMMENT '实际收费金额',
      `assetsFee` float(10,3) DEFAULT null COMMENT '总资产账面值',
       `noAssetsFee` boolean DEFAULT false COMMENT '无总资产账面值',
       `debtFee` float(10,3) DEFAULT null COMMENT '总负债面值',
       `noDebtFee` boolean DEFAULT false COMMENT '无总负债面值',
     `netAssets` float(10,3) DEFAULT null COMMENT '净资产账面值',
       `noNetAssets` boolean DEFAULT false COMMENT '无净资产账面值',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;